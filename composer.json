{
  "name": "uay/y-entity-generator-bundle",
  "type": "symfony-bundle",
  "description": "Entity generator for Symfony applications",
  "keywords": [
    "entity",
    "generator"
  ],
  "homepage": "https://github.com/uay/YEntityGeneratorBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Project Contributors",
      "homepage": "https://github.com/uay/YEntityGeneratorBundle/blob/master/CONTRIBUTORS.md"
    }
  ],
  "require": {
    "php": "^7.2",
    "jawira/plantuml-encoding": "^1.0",
    "symfony/framework-bundle": "~5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Uay\\YEntityGeneratorBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTestBundle\\": "tests/Fixtures/AppTestBundle/",
      "Uay\\YEntityGeneratorBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
